/* packages/landlordMode/pages/housing/housing.wxss */
page{
  margin-bottom: 160rpx;
}

.container{
  width: 85%;
  margin: 0 auto;
  font-size: 26rpx;
}

.hover{
  opacity: 0.6;
}

.h2{
  color: rgb(0, 132, 137);
  font-size: 30rpx;
  font-weight: bold;
}

.new{
  margin: 40rpx 0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 6rpx;
  box-shadow: 0 0 6rpx lightgray;
}

.new >view{
  display: inline-block;
  margin-left: 20rpx;
}

.new .icon-jia1{
  font-size: 40rpx;
  font-weight: bold;
  color: rgb(0, 132, 137);
}

.group{
  width: 100%;
  border-radius: 6rpx;
  border: 1rpx solid rgb(240, 240, 240);
  margin-bottom: 40rpx;
}

.group .group-card, .navigate{
  width: 85%;
  margin: 0 auto;
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
}

.group .group-card{
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}

.group .navigate{
  margin-bottom: 40rpx;
}

.group .group-card image{
  width: 40%;
  height: 140rpx;
}
.group .group-card >view{
  width: 30%;
}

.group .group-card >view view{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-top: 10rpx;
}

.group .navigate .text{
  color: rgb(0, 132, 137);
}


