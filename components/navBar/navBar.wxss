/* components/navBar/navBar.wxss */
@import '/lib/iconfont/iconfont.wxss';
.navbar {
  position: fixed;
  z-index: 999;
}
 
.navbar-wrap {
  position: absolute;
  border: .5px solid lightgrey;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(255, 255, 255, .5);
}

.navbar-wrap view{
  font-size: 20px;
  color: black;
}

.navbar-wrap :nth-child(2){
  color: lightgrey;
}
